{
  "name": "backbase",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dist": "webpack -p",
    "test": "karma start karma.conf.js",
    "test-ci": "karma start karma.conf.sauce.js --no-auto-watch --single-run",
    "release-patch": "git checkout master && npm version patch && git push origin master",
    "release-minor": "git checkout master && npm version minor && git push origin master",
    "release-major": "git checkout master && npm version major && git push origin master",
    "docco": "./node_modules/.bin/docco src/**/*.js --layout classic --output docs/docco",
    "jsdoc": "./node_modules/.bin/jsdoc --readme README.md src -r --destination docs/jsdoc --configure .jsdoc.json",
    "jsdoc2md": "jsdoc2md 'src/*.js' > docs/framework.md && jsdoc2md 'src/utils/*.js' > docs/utilities.md"
  },
  "devDependencies": {
    "chai": "^3.3.0",
    "docco": "^0.7.0",
    "istanbul-instrumenter-loader": "^0.1.3",
    "jscs-jsdoc": "^1.2.0",
    "jsdoc": "^3.3.3",
    "karma": "^0.13.14",
    "karma-chai": "*",
    "karma-coverage": "^0.5.3",
    "karma-html2js-preprocessor": "^0.1.0",
    "karma-mocha": "*",
    "karma-phantomjs-launcher": "*",
    "karma-sauce-launcher": "^0.3.0",
    "karma-sinon": "*",
    "karma-spec-reporter": "0.0.20",
    "karma-webpack": "^1.7.0",
    "webpack": "^1.12.2"
  },
  "dependencies": {
    "flux": "^2.1.1"
  }
}
